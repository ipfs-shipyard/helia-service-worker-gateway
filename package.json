{
  "name": "helia-service-worker-gateway",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Bundle Helia with Webpack",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "analyze-bundle": "webpack --env analyze",
    "clean": "aegir clean dist dist-tsc",
    "dep-check": "aegir dep-check",
    "lint": "aegir lint",
    "lint:fix": "aegir lint --fix",
    "build": "run-s build:esm build:webpack",
    "prebuild:esm": "copyfiles src/*.svg src/**/*.svg src/*.css src/**/*.css dist-tsc",
    "build:esm": "tsc",
    "build:webpack": "webpack --env production",
    "serve": "webpack serve --mode=development",
    "serve:prod": "webpack serve --mode=production",
    "start": "npm run serve",
    "test": "run-s test:iso 'test:node -- -b false'",
    "test:iso": "aegir test -f dist-tsc/test/**/*.spec.js",
    "test:browsers": "playwright test -c playwright.config.js",
    "test:chrome": "playwright test -c playwright.config.js --project chromium",
    "test:firefox": "playwright test -c playwright.config.js --project firefox",
    "test:node": "aegir test -t node -f dist-tsc/test/node.js --cov",
    "postinstall": "patch-package"
  },
  "browser": "./dist/src/index.js",
  "browserslist": [
    "last 1 Chrome version"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "dependencies": {
    "@helia/ipns": "^6.0.1",
    "@helia/verified-fetch": "^1.2.0",
    "@libp2p/logger": "^4.0.6",
    "@sgtpooki/file-type": "^1.0.1",
    "debug": "^4.3.4",
    "multiformats": "^11.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@playwright/test": "^1.42.1",
    "@types/react": "^18.0.31",
    "aegir": "^42.2.2",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.10.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.8.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "playwright": "^1.42.1",
    "rimraf": "^4.4.1",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.77.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.8.0"
  },
  "sideEffects": [
    "*.css"
  ]
}
